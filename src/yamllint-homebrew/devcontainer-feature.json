{
    "id": "yamllint-homebrew",
    "version": "1.0.0",
    "name": "Yamllint (via Homebrew)",
    "documentationURL": "https://github.com/paul-gilber/devcontainer-features/tree/main/src/yamllint-homebrew",
    "description": "Yamllint is a linter for YAML files",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/homebrew-package"
    ]
}
