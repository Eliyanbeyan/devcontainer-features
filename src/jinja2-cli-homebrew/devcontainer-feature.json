{
    "id": "jinja2-cli-homebrew",
    "version": "1.0.0",
    "name": "Jinja2 CLI (via Homebrew)",
    "documentationURL": "https://github.com/paul-gilber/devcontainer-features/tree/main/src/jinja2-cli-homebrew",
    "description": "Jinja2 is a full-featured template engine for Python. It has full unicode support, an optional integrated sandboxed execution environment, widely used and BSD licensed",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/homebrew-package"
    ]
}
