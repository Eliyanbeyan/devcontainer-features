{
    "id": "openshift-cli-homebrew",
    "version": "1.0.0",
    "name": "OpenShift CLI (via Homebrew)",
    "documentationURL": "https://github.com/paul-gilber/devcontainer-features/tree/main/src/openshift-cli-homebrew",
    "description": "OpenShift is a unified platform to build, modernize, and deploy applications at scale",
    "options": {
        "version": {
            "default": "latest",
            "description": "Select the version to install.",
            "proposals": [
                "latest"
            ],
            "type": "string"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers-contrib/features/homebrew-package"
    ]
}
